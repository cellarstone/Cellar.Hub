{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "poseidon-cli"
  },
  "apps": [
    {
      "root": "SPA",
      "outDir": "wwwroot",
      "assets": [
        "assets"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "styles": [
        "../node_modules/font-awesome/css/font-awesome.min.css",
        "../node_modules/material-design-icons/iconfont/material-icons.css",
        "../node_modules/primeng/resources/primeng.min.css",
        "../node_modules/fullcalendar/dist/fullcalendar.min.css",
        "../node_modules/quill/dist/quill.snow.css",
        "../node_modules/nanoscroller/bin/css/nanoscroller.css",
        "styles.scss"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.js",
        "../node_modules/nanoscroller/bin/javascripts/jquery.nanoscroller.js",
        "../node_modules/moment/moment.js",
        "../node_modules/chart.js/dist/Chart.js",
        "../node_modules/fullcalendar/dist/fullcalendar.js",
        "../node_modules/quill/dist/quill.js",
        "../node_modules/jquery/dist/jquery.min.js",
        "../SPA/assets/MorphSVGPlugin.js",
        "../SPA/assets/CustomBounce.js",
        "../SPA/assets/CustomEase.js",
        "../SPA/assets/Physics2DPlugin.js",
        "../SPA/assets/PhysicsPropsPlugin.js",
        "../SPA/assets/DrawSVGPlugin.js",
        "../SPA/assets/ScrambleTextPlugin.js",
        "../SPA/assets/SplitText.js",
        "../SPA/assets/ThrowPropsPlugin.js"
      ],
      "environmentSource": "SPA/environments/environment.ts",
      "environments": {
        "source": "SPA/environments/environment.ts",
        "dev": "SPA/environments/environment.ts",
        "prod": "SPA/environments/environment.prod.ts"
      }
    }
  ],
  "lint": [
    {
      "project": "SPA/tsconfig.json"
    }
  ],
  "defaults": {
    "styleExt": "scss",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}