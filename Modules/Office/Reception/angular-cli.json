{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "project": {
      "name": "cellar-hub-office-reception"
    },
    "apps": [
      {
        "root": "SPA",
        "outDir": "wwwroot",
        "assets": [
          "assets",
          "favicon.ico"
        ],
        "index": "index.html",
        "main": "main.ts",
        "polyfills": "polyfills.ts",
        "tsconfig": "tsconfig.json",
        "prefix": "app",
        "styles": [
            "../node_modules/bootstrap/dist/css/bootstrap.min.css",
            "../node_modules/font-awesome/css/font-awesome.css",
            "styles.scss"
          ],
        "scripts": [],
        "environmentSource": "SPA/environments/environment.ts",
        "environments": {
          "source": "SPA/environments/environment.ts",
          "dev": "SPA/environments/environment.ts",
          "prod": "SPA/environments/environment.prod.ts"
        }
      }
    ],
    "lint": [
      {
        "project": "SPA/tsconfig.json"
      }
    ],
    "defaults": {
      "styleExt": "scss",
      "prefixInterfaces": false,
      "inline": {
        "style": false,
        "template": false
      },
      "spec": {
        "class": false,
        "component": true,
        "directive": true,
        "module": false,
        "pipe": true,
        "service": true
      }
    }
  }